{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "messageformat",
      "camelcase": "MessageFormat",
      "title": "messageformat",
      "scope": "source.messageformat",
      "file-types": [
        "xml",
        "msg",
        "msg.js",
        "msg.json",
        "messages"
      ],
      "injection-regex": "^messageformat$",
      "class-name": "TreeSitterMessageformat"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "messageformat parser",
    "authors": [
      {
        "name": "Danny Gleckle",
        "email": "daniel.gleckler@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/bearfriend/tree-sitter-messageformat"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
